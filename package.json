{
  "name": "react-app",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production npm-run-all build:clean build:mkdir build:copy build:webpack",
    "serve": "serve build -p 8000",
    "###################### Build ######################": "",
    "build:clean": "rimraf ./build",
    "build:mkdir": "mkdirp build",
    "build:copy": "ncp ./public ./build/",
    "build:webpack": "webpack --config webpack.prod.js",
    "###################### Linters ######################": "",
    "lint:js": "eslint --config ./.eslintrc --ignore-path ./.gitignore --format table --quiet --color \"./**/*.js\"",
    "###################### NPM Specific Scripts ######################": "",
    "packages:outdated": "npm outdated",
    "packages:update": "npm update",
    "packages:audit": "npm audit",
    "packages:fix": "npm audit fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^6.0.3",
    "html-webpack-plugin": "^5.5.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.19.1",
    "serve": "^11.3.0",
    "webpack-dev-server": "^3.10.3"
  }
}
